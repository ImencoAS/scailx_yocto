{
    "terminal.integrated.profiles.linux": {
        "kas": {
            "path": "bash",
            "args": ["--init-file", "scripts/videology-setup-env"],
            "icon": "terminal-debian"
        },
        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        }
    },
    {
        "bitbake.machine": "scailx-imx8mp",
        "bitbake.distro": "scailx",
        "bitbake.oeInitBuildEnvPath": "${workspaceFolder}/poky/oe-init-build-env",
        "bitbake.bitbakePath": "${workspaceFolder}/poky/bitbake/bin/bitbake/",
        "bitbake.buildPath": "${workspaceFolder}/build",
    },
    "terminal.integrated.defaultProfile.linux": "kas",
    "search.exclude": {
        "**/.repo": true,
        "**/build/**": true,
        "**/downloads": true,
        "**/sstate-cache": true
    },
    "files.exclude": {
        "build/cache/**": true,
        "build/tmp/**": true,
        "build/sstate-cache/**": true,
        "downloads/**": true,
        "*ctags*":  true,
        "sstate-cache*/**": true,
    },
    "C_Cpp.files.exclude": {
        "build/**": true,
    },
    "search.followSymlinks": false,
    "git-blame.gitWebUrl": "",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        ".repo/**": true,
        "build/**": true,
        "**/build_wsl/**": true,
        "**/downloads/**": true,
        "**/target": true,
        "**/sstate-cache/**": true
    },
    "search.useIgnoreFiles": false
}
